{
  "schemaVersion": 1,
  "id": "smart_survival",
  "version": "${version}",
  "name": "smart_survival",
  "description": "Smart Survival is a Fabric mod for Minecraft 1.21.11 that adds campfire comfort, sleep quality effects and nutritional food buffs to enhance survival.",
  "authors": ["Nitin"],
  "contact": {
    "sources": "https://github.com/yourname/smart-survival"
  },
  "license": "GPL-3.0-only",
  "icon": "assets/smart_survival/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.nitin.smartsurvival.smart_survival.client.Smart_survivalDataGenerator"
    ],
    "client": [
      "com.nitin.smartsurvival.smart_survival.client.Smart_survivalClient"
    ],
    "main": [
      "com.nitin.smartsurvival.smart_survival.Smart_survival"
    ]
  },
  "mixins": [
    "smart_survival.mixins.json",
    {
      "config": "smart_survival.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}
